{
  "name": "notey-notes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env DEBUG=notes:* SEQUELIZE_CONNECT=models/sequelize-sqlite.yaml NOTES_MODEL=sequelize USER_SERVICE_URL=http://localhost:5858 node ./app.mjs",
    "fs-start": "cross-env DEBUG=notes:* PORT=3000 NOTES_MODEL=fs node ./app.mjs",
    "fs-server1": "cross-env NOTES_MODEL=fs PORT=3001 node ./app.mjs",
    "fs-server2": "cross-env NOTES_MODEL=fs PORT=3002 node ./app.mjs",
    "level-start": "cross-env DEBUG=notes:* PORT=3000 NOTES_MODEL=level node ./app.mjs",
    "sqlite3-setup": "sqlite3 notes.sqlite --init ./models/schema-sqlite3.sql",
    "sqlite3-start": "cross-env SQLITE_FILE=notes.sqlite DEBUG=notes:* NOTES_MODEL=sqlite3 node ./app.mjs",
    "sqlite3-server1": "cross-env SQLITE_FILE=notes.sqlite NOTES_MODEL=sqlite3 PORT=3001 node ./app.mjs",
    "sqlite3-server2": "cross-env SQLITE_FILE=notes.sqlite NOTES_MODEL=sqlite3 PORT=3002 node ./app.mjs",
    "sequelize-start": "cross-env DEBUG=notes:* SEQUELIZE_CONNECT=models/sequelize-sqlite.yaml NOTES_MODEL=sequelize node ./app.mjs",
    "sequelize-server1": "cross-env SEQUELIZE_CONNECT=models/sequelizesqlite.yaml NOTES_MODEL=sequelize PORT=3001 node ./app.mjs",
    "sequelize-server2": "cross-env SEQUELIZE_CONNECT=models/sequelizesqlite.yaml NOTES_MODEL=sequelize PORT=3002 node ./app.mjs"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bootstrap": "4.5.x",
    "capture-console": "^1.0.2",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "1.17.x",
    "feather-icons": "4.25.x",
    "fs-extra": "^11.2.0",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "jquery": "3.5.x",
    "js-yaml": "3.13.x",
    "level": "^6.0.1",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport.socketio": "3.7.x",
    "popper.js": "1.16.x",
    "rotating-file-stream": "^3.2.1",
    "sequelize": "^6.37.1",
    "session-file-store": "1.4.x",
    "socket.io": "^2.5.0",
    "sqlite3": "^5.1.7",
    "superagent": "5.2.x"
  }
}
